@using SFMS.ReportHelpers
@model IList<StudentDetail>
                <head>
                    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
                    <title>
                        Report
                    </title>
                </head>
    <body>
        <form action="~/report/StudentDetailFromCodeToCode" method="get" target="_blank">
            <div class="mb-3">
                <label class="form-label">From Code</label>
                <select name="FromCode">
                    @foreach(var item in Model)
                    {
                    <option value="@item.Code">@item.Code</option>
                    }
                </select>
            </div>
            <div class="mb-3">
                <label class="form-label">To Code</label>
                <select name="ToCode">
                    @foreach(var item in Model)
                    {
                    <option value="@item.Code">@item.Code</option>
                    }
                </select>
            </div>
            <div class="mb-3">
                <div class="row">
                    <div class="col">
                        <input type="submit" class="btn btn-success" value="Show" />
                    </div>
                    <div class="col">
                        <input type="reset" class="btn btn-secondary" value="Cancel" />
                    </div>
                </div>
            </div>
        </form>
    </body>
